extruder.hotend.retract_feedrate                35            # Retract feedrate in mm/sec
extruder.hotend.retract_recover_length          0             # Additional length for recover
extruder.hotend.retract_recover_feedrate        7             # Recover feedrate in mm/sec (should be less than retract feedrate)
#extruder.hotend.retract_zlift_length            0.600         # Z-lift on retract in mm, 0 disables
#extruder.hotend.retract_zlift_feedrate          300           # Z-lift feedrate in mm/min (Note mm/min NOT mm/sec)

delta_current                                    0.90         # First extruder stepper motor current


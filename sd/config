custom_menu.cal1.command                G0X122.5Y5F7000      #

custom_menu.cal2.enable                      true            #
custom_menu.cal2.name                       Check_Back_Right #
custom_menu.cal2.command                G0X240Y240F7000      #

custom_menu.cal3.enable                      true            #
custom_menu.cal3.name                        Check_Back_Left #
custom_menu.cal3.command                G0X5Y240F7000        #

custom_menu.cal4.enable                      true            #
custom_menu.cal4.name                        Check_Center    #
custom_menu.cal4.command                G0X122.5Y122.5F7000  #

custom_menu.test0.enable                     true            #
custom_menu.test0.name                       Speed_300_\     #
custom_menu.test0.command            G1X245Y0F18000|G1X0Y245 #

custom_menu.test1.enable                     true            #
custom_menu.test1.name                       Speed_300_/     #

switch.resume.input_pin                  	2.12             # Pin where resume button is connected
switch.resume.output_on_command          	resume           # Resume command

after_suspend_gcode        G91_G0Z1_G90_G0X120Y0    		 # G91_G0E-100_G0Z1_G90_G0X120Y0 Gcode to run after suspend, retract then get head out of way
before_resume_gcode        		M117       					 # G91_G1E1_G90 Gcode to run after temp is reached but before resume - do a prime

#msd_disable                                 false            # Disable the MSD (USB SDCARD), see http://smoothieware.org/troubleshooting#disable-msd
#dfu_enable                                  false            # For linux developers, set to true to enable DFU

# Only needed on a smoothieboard
# See http://smoothieware.org/currentcontrol
currentcontrol_module_enable                 true            # Control stepper motor current via the configuration file
digipotchip                                  mcp4728         #  
mcp4726_adress                               0xC6            #
mcp4728_adress                               0xC0            #